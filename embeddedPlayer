bandcamp scraper:


var bandcampScraper = require("bandcamp-scraper")

const albumUrl = 'https://nilodude.bandcamp.com/album/bounded-input-bouncing-output-ind005'
bandcampScraper.getAlbumInfo(albumUrl, function (error, albumInfo) {
  if (error) {
    console.log(error)
  } else {
    console.log(albumInfo.raw.trackinfo[0].file['mp3-128'])
  }
})